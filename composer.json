{
    "name": "modera/backend-security-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides UI section for MF that makes it possible to manage users/groups/permissions.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Vizel",
            "email": "sergei.vizel@modera.org"
        },
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~2.6",
        "symfony/swiftmailer-bundle": "~2.3",
        "modera/security-bundle": "~2.0",
        "modera/backend-tools-bundle": "~2.0",
        "modera/mjr-integration-bundle": "~1.1|~2.0",
        "modera/backend-languages-bundle": "~2.0",
        "modera/translations-bundle": "~1.0|~2.0",
        "modera/activity-logger-bundle": "~1.0|~2.0",
        "modera/server-crud-bundle": "~2.0"
    },
    "require-dev": {
        "modera/foundation-bundle": "~1.0|~2.0",
        "phake/phake": "2.*",
        "phpunit/phpunit": "3.*"
    },
    "autoload": {
        "psr-0": { "Modera\\BackendSecurityBundle": "" }
    },
    "target-dir": "Modera/BackendSecurityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
