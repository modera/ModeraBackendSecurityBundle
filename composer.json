    "require": {
        "php": ">=7.4",
        "modera/security-bundle": "^4.0",
        "modera/server-crud-bundle": "^4.0",
        "modera/translations-bundle": "^4.0",
        "modera/backend-tools-bundle": "^4.0",
        "modera/activity-logger-bundle": "^4.0",
        "modera/mjr-integration-bundle": "^4.0",
        "modera/backend-languages-bundle": "^4.0"
    },
    "require-dev": {
        "phake/phake": "^4.0",
        "phpunit/phpunit": "^6.5",
        "symfony/mailer": "^4.4|^5.0",
        "modera/foundation-bundle": "^4.0"
    },
    "autoload": {
        "psr-4": { "Modera\\BackendSecurityBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
